<div class="col"> 
  <div class="card mx-5 mt-5">
    <img src="/images/funds/{{funds.[0].imageLink}}" alt="{{funds.[0].description}}" width="85%" height="auto" class="card-img-top">
    <div class="card-body">
      <h5 class="card-title">{{name}}</h5>
      <p class="card-text">{{description}}</p>
      <a href="/category/{{id}}" class="btn btn-primary">View Fundraiser!</a>
    </div>
  </div>
</div>